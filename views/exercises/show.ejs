<h2 class="page-header"><%= exercise.name %></h2>
<div class="btn-row">
    <a class="btn btn-primary" href="/exercises/<%= exercise.id %>/edit">Edit</a>
    <%- include('../partials/deleteForm', {url: `/exercises/${exercise.id}`}) %>
</div>
<% if(musclegroupsByExercise.length > 0) { %>
    <h2 class="page-header" >Muclegroups By Exercise</h2>
    <% musclegroupsByExercise.forEach(musclegroup => { %>
        <a href="/musclegroups/<%= musclegroup.id %>"> 
            <img height="150" width="100" src="<%= musclegroup.coverImagepath %>">
        </a>
    <% }) %>
<% } %>

